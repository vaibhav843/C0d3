import array
num=array.array('i', [])
n=int(input("enter number of element you want to add to array"))
for i in range(n):
	ele=int(input("enter element"))
	num.append(ele)
min=max=num[0]
for i in range(1,len(num)):
	if num[i]<min:
		min=num[i]
	if num[i]>max:
		max=num[i]
print("max",max,"min",min)
